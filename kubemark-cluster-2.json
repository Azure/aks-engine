{
    "apiVersion": "vlabs",
    "location": "",
    "properties": {
        "orchestratorProfile": {
            "orchestratorType": "Kubernetes",
            "orchestratorRelease": "1.17",
            "kubernetesConfig": {
                "networkPolicy": "none",
                "cloudProviderRateLimitQPS": 6,
                "cloudProviderRateLimitBucket": 20,
                "controllerManagerConfig": {
                    "--feature-gates": "CSIInlineVolume=true,LocalStorageCapacityIsolation=true,ServiceNodeExclusion=true",
                    "--profiling": "true"
                },
                "apiServerConfig": {
                    "--enable-admission-plugins": "NamespaceLifecycle,LimitRanger,ServiceAccount,DefaultStorageClass,ResourceQuota,AlwaysPullImages",
                    "--profiling": "true"
                },
                "schedulerConfig": {
                    "--profiling": "true"
                }
            }
        },
        "masterProfile": {
            "count": 1,
            "dnsPrefix": "test-ssh",
            "vmSize": "Standard_DS2_v2",
            "extensions": [
                {
                    "name": "config-kubemark-master"
                }
            ],
            "sshAlternativePort": 13579
        },
        "extensionProfiles": [
            {
                "name": "config-kubemark-master",
                "version": "v1",
                "extensionParameters": "parameters",
                "rootURL": "https://raw.githubusercontent.com/nilo19/kubemark-performance-tests-on-azure/master/"
            }
        ],
        "linuxProfile": {
            "adminUsername": "kubernetes",
            "ssh": {
                "publicKeys": [
                    {
                        "keyData": "ssh-rsa  pomelo@niqideMacBook-Pro.local"
                    }
                ]
            }
        },
        "servicePrincipalProfile": {
            "clientID": "672fa89a-8e10-4201-a3bd-0126f0dc8730",
            "secret": "cf442a6e-2391-4257-9265-6222d4880977"
        }
    }
}