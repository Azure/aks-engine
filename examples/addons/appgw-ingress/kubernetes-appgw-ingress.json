      "orchestratorType": "Kubernetes",
      "kubernetesConfig": {
        "networkPlugin": "azure",
        "addons": [
          {
            "name": "appgw-ingress",
            "enabled": true,
            "config": {
              "appgw-subnet": "<subnet CIDR>",
              "appgw-sku": "WAF_v2"
            }
          }
        ]
      }
    },
    "masterProfile": {
      "count": 1,
      "dnsPrefix": "",
