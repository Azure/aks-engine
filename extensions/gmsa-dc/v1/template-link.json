{
    "name": "gmsa-dc",
    "type": "Microsoft.Resources/deployments",
    "apiVersion": "[variables('apiVersionDeployments')]",
    "dependsOn": [
        "[concat('Microsoft.Compute/virtualMachines/', EXTENSION_TARGET_VM_NAME_PREFIX)]"
	],
    "properties": {
        "mode": "Incremental",
        "templateLink": {
            "uri": "https://raw.githubusercontent.com/JeremyWx/aks-engine/master/extensions/gmsa-dc/v1/template.json",
            "contentVersion": "1.0.0.0"
        },
        "parameters": {
             "apiVersionDeployments": {
                "value": "[variables('apiVersionDeployments')]"
            },
            "targetVMName": {
                "value": "[concat(EXTENSION_TARGET_VM_NAME_PREFIX)]"
            },
            "targetVMType": {
                "value": "EXTENSION_TARGET_VM_TYPE"
            }
        }
    }
}