{
	"name": "Go for aks-engine",
	"image": "quay.io/deis/go.dev:v1.22.1",
	"extensions": [
		"ms-vscode.go"
	],
	"runArgs": [
		"--cap-add=SYS_PTRACE",
		"--security-opt",
		"seccomp=unconfined"
	],
	"workspaceMount": "type=bind,src=.,dst=/go/src/github.com/Azure/aks-engine",
  "workspaceFolder": "/go/src/github.com/Azure/aks-engine",
	"settings": {
		"go.gopath": "/go"
	}
}
