            "config": {
              "dockerProviderVersion": "8.0.0-2",
              "omsAgentVersion": "1.10.0.1"
            }
          },
          {
            "name": "azure-cni-networkmonitor",
            "enabled": true,
            "containers": [
              {
                "name": "azure-cni-networkmonitor",
                "image": "containernetworking/networkmonitor:v0.0.5"
              }
            ]
          },
          {
            "name": "azure-npm-daemonset",
            "enabled": false,
            "containers": [
              {
                "name": "azure-npm-daemonset"
              }
            ]
          },
          {
            "name": "ip-masq-agent",
            "enabled": true,
            "containers": [
              {
                "name": "ip-masq-agent",
                "image": "k8s.gcr.io/ip-masq-agent-amd64:v2.3.0",
                "cpuRequests": "50m",
                "memoryRequests": "50Mi",
                "cpuLimits": "50m",
                "memoryLimits": "250Mi"
